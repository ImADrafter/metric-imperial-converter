{
	"name": "my-hyperdev-app",
	"version": "0.0.1",
	"description": "What am I about?",
	"main": "server.js",
	"babel": {
		"presets": [
			"@babel/preset-env"
		]
	},
	"nodemonConfig": {
		"exec": "npm run dev",
		"watch": [
			"./server/*"
		]
	},
	"scripts": {
		"transpile": "babel ./server --out-dir dist-server",
		"clean": "rimraf dist-server",
		"build": "npm-run-all clean transpile",
		"dev": "npm run clean && npm run transpile && node ./dist-server/server.js",
		"prod": "NODE_ENV=production npm-run-all build server",
		"watch:dev": "nodemon"
	},
	"dependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.6",
		"@babel/preset-env": "^7.8.6",
		"body-parser": "^1.15.2",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"cors": "^2.8.1",
		"express": "^4.14.0",
		"helmet": "^3.21.3",
		"mocha": "^7.0.1",
		"mongodb": "^3.5.3",
		"rimraf": "^3.0.2",
		"ts-node": "^8.6.2",
		"zombie": "^6.1.4"
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"custom-env": "^2.0.1",
		"eslint": "^6.8.0",
		"eslint-config-node": "^4.0.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-prettier": "^3.1.2",
		"nodemon": "^2.0.2",
		"prettier": "^1.19.1",
		"typescript": "3.4.3"
	},
	"engines": {
		"node": "13.8.0"
	},
	"repository": {
		"type": "git",
		"url": "https://hyperdev.com/#!/project/welcome-project"
	},
	"keywords": [
		"node",
		"hyperdev",
		"express"
	],
	"license": "MIT"
}
